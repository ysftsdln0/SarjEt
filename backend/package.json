{
  "name": "sarjet-backend",
  "version": "1.0.0",
  "description": "SarjEt Backend API - Electric Vehicle Charging Station Service",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "build": "npm run prisma:generate && npm run prisma:deploy",
    "prisma:generate": "prisma generate",
