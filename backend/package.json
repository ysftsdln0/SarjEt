    "jsonwebtoken": "^9.0.2",
    "joi": "^17.9.2",
    "express-rate-limit": "^6.8.1",
    "compression": "^1.7.4",
    "@prisma/client": "^5.1.1",
    "axios": "^1.4.0",
    "node-cron": "^3.0.2",
    "winston": "^3.10.0",
    "redis": "^4.6.7"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "prisma": "^5.1.1",
